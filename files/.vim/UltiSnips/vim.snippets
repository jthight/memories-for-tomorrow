snippet ifh "Vim if has() statment"
if has("${1:condition}")$2
  ${3:${VISUAL:pass}}
endif$0
endsnippet

snippet has "add has condition to if statement"
has("${1:condition}")$0
endsnippet

snippet if "Vim if statment"
if ${1:condition}$2
  ${3:${VISUAL:pass}}
endif$0
endsnippet

snippet fun "function"
function ${1:Name}($2)
  ${3:${VISUAL:pass}}$4
endfunction$0
endsnippet

